{
  "name": "front-end-stack",
  "version": "0.12.0",
  "private": true,
  "config": {
    "context_root": "/",
    "dist_uri": "/assets/",
    "src_dir_path": "src/",
    "test_dir_path": "src/**/*-spec.js",
    "dist_dir_path": "dist/assets/",
    "entry_file_path": "dist/index.html",
    "report_dir_path": "reports/"
  },
  "scripts": {
    "test": "node ./scripts/test",
    "test:watch": "nodemon --exec 'yarn test'",
    "build": "webpack --mode production --progress",
    "ci": "node ./scripts/ci",
    "reinstall": "yarn cache clean && rimraf node_modules/ && yarn",
    "start": "webpack-dev-server --mode development --config webpack.dev.config.js --progress",
    "stats": "webpack --profile --json > stats.json",
    "flow": "flow stop && flow"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "history": "4.7.2",
    "immutable": "4.0.0-rc.9",
    "material-ui": "1.0.0-beta.38",
    "material-ui-icons": "1.0.0-beta.36",
    "moment": "2.21.0",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "3.7.2",
    "redux-observable": "0.18.0",
    "reselect": "3.0.1",
    "rxjs": "5.5.7",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "autoprefixer": "8.5.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-istanbul": "4.1.6",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "clean-webpack-plugin": "0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "0.28.11",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.47.1",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.8.2",
    "file-loader": "1.1.11",
    "flow-bin": "0.72.0",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "4.2.0",
    "jsdom": "11.10.0",
    "mini-css-extract-plugin": "0.4.0",
    "mocha": "5.2.0",
    "mocha-junit-reporter": "1.17.0",
    "nock": "9.2.6",
    "nodemon": "1.17.5",
    "nyc": "11.8.0",
    "postcss-loader": "2.1.5",
    "react-test-renderer": "16.3.2",
    "rimraf": "2.6.2",
    "sinon": "5.0.7",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "1.0.1",
    "webpack": "4.8.3",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "3.1.4"
  }
}
