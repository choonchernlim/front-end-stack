{
  "name": "front-end-stack",
  "private": true,
  "config": {
    "context_root": "/",
    "dist_uri": "/assets/",
    "src_dir_path": "src/",
    "test_dir_path": "src/**/*-spec.js",
    "dist_dir_path": "dist/assets/",
    "entry_file_path": "dist/index.html",
    "report_dir_path": "reports/"
  },
  "scripts": {
    "test": "node ./scripts/test",
    "test:watch": "nodemon --exec 'npm test'",
    "build": "webpack",
    "ci": "npm run ci:clean && npm run ci:test && npm run ci:coverage",
    "ci:clean": "node ./scripts/ci-clean",
    "ci:coverage": "node ./scripts/ci-coverage",
    "ci:test": "node ./scripts/ci-test",
    "reinstall": "npm cache clean && rimraf \"node_modules/!(rimraf|.bin)\" && npm install",
    "start": "webpack-dev-server --config webpack.dev.config.js",
    "stats": "webpack --profile --json > stats.json"
  },
  "dependencies": {
    "history": "3.2.0",
    "immutable": "3.8.1",
    "isomorphic-fetch": "2.2.1",
    "material-ui": "0.16.4",
    "moment": "2.17.0",
    "radium": "0.18.1",
    "radium-grid": "2.0.3",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-redux": "4.4.6",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.7",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.6.0",
    "redux-saga": "0.13.0",
    "reselect": "2.5.4"
  },
  "devDependencies": {
    "autoprefixer": "6.5.3",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.8",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "clean-webpack-plugin": "0.1.14",
    "css-loader": "0.26.0",
    "enzyme": "2.6.0",
    "eslint": "3.11.1",
    "eslint-config-airbnb": "13.0.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.7.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.1",
    "image-webpack-loader": "3.0.0",
    "isparta": "4.0.0",
    "jsdom": "9.8.3",
    "json-loader": "0.5.4",
    "mocha": "3.2.0",
    "mocha-junit-reporter": "1.12.1",
    "nock": "9.0.2",
    "node-sass": "3.13.0",
    "nodemon": "1.11.0",
    "postcss-loader": "1.1.1",
    "react-addons-perf": "15.4.1",
    "react-addons-test-utils": "15.4.1",
    "rimraf": "2.5.4",
    "roboto-fontface": "0.6.0",
    "sass-loader": "4.0.2",
    "sinon": "1.17.6",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.9"
  }
}
