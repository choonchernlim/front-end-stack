{
  "name": "front-end-stack",
  "private": true,
  "config": {
    "context_root": "/",
    "dist_uri": "/assets/",
    "src_dir_path": "src/",
    "test_dir_path": "src/**/*-spec.js",
    "dist_dir_path": "dist/assets/",
    "entry_file_path": "dist/index.html",
    "report_dir_path": "reports/"
  },
  "scripts": {
    "test": "node ./scripts/test",
    "test:watch": "nodemon --exec 'npm test'",
    "build": "webpack --progress",
    "ci": "node ./scripts/ci",
    "reinstall": "npm cache clean && rimraf \"node_modules/!(rimraf|.bin)\" && npm install",
    "start": "webpack-dev-server --config webpack.dev.config.js --progress",
    "stats": "webpack --profile --json > stats.json",
    "flow": "flow"
  },
  "dependencies": {
    "history": "3.2.0",
    "immutable": "3.8.1",
    "isomorphic-fetch": "2.2.1",
    "material-ui": "0.17.0",
    "moment": "2.17.1",
    "radium": "0.18.1",
    "radium-grid": "2.0.3",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.8",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.6.0",
    "redux-saga": "0.14.3",
    "reselect": "2.5.4"
  },
  "devDependencies": {
    "autoprefixer": "6.7.5",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.3.2",
    "babel-plugin-istanbul": "4.0.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "clean-webpack-plugin": "0.1.15",
    "css-loader": "0.26.1",
    "enzyme": "2.7.1",
    "eslint": "3.16.1",
    "eslint-config-airbnb": "14.1.0",
    "eslint-loader": "1.6.3",
    "eslint-plugin-flowtype": "2.30.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.0",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "file-loader": "0.10.0",
    "flow-bin": "0.39.0",
    "html-webpack-plugin": "2.28.0",
    "image-webpack-loader": "3.2.0",
    "jsdom": "9.11.0",
    "mocha": "3.2.0",
    "mocha-junit-reporter": "1.13.0",
    "nock": "9.0.6",
    "node-sass": "4.5.0",
    "nodemon": "1.11.0",
    "nyc": "10.1.2",
    "postcss-loader": "1.3.1",
    "react-addons-perf": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "redux-saga-test-plan": "2.2.1",
    "rimraf": "2.6.0",
    "roboto-fontface": "0.7.0",
    "sass-loader": "6.0.2",
    "sinon": "1.17.7",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "exclude": [
      "scripts/",
      "webpack.**.js",
      "**/*spec.js",
      "**/__tests__/**",
      "**/node_modules/**"
    ],
    "reporter": [
      "html",
      "cobertura"
    ],
    "sourceMap": false,
    "instrument": false,
    "all": true
  }
}
