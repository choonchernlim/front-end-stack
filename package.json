{
  "name": "front-end-stack",
  "version": "0.16.0",
  "private": true,
  "sideEffects": false,
  "config": {
    "context_root": "/",
    "dist_uri": "/assets/",
    "src_dir_path": "src/",
    "test_dir_path": "src/**/*-spec.js",
    "dist_dir_path": "dist/assets/",
    "entry_file_path": "dist/index.html",
    "report_dir_path": "reports/"
  },
  "scripts": {
    "test": "node ./scripts/test",
    "test:watch": "nodemon --exec 'yarn test'",
    "build": "webpack",
    "ci": "node ./scripts/ci",
    "reinstall": "rimraf node_modules/ && yarn --network-timeout 1000000",
    "start": "webpack-dev-server --config webpack.dev.config.js",
    "stats": "webpack --profile --json > stats.json",
    "flow": "flow --color always",
    "flow:restart": "flow stop && yarn flow",
    "prettier": "prettier --write '**/*.{js,json,md}'"
  },
  "dependencies": {
    "@material-ui/core": "3.9.3",
    "@material-ui/icons": "3.0.2",
    "classnames": "2.2.6",
    "connected-react-router": "6.3.2",
    "date-fns": "1.30.1",
    "history": "4.9.0",
    "immer": "2.1.4",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "6.0.1",
    "react-router-dom": "5.0.0",
    "recompose": "0.30.0",
    "redux": "4.0.1",
    "redux-observable": "1.1.0",
    "reselect": "4.0.0",
    "rxjs": "6.4.0",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.4",
    "@babel/plugin-proposal-class-properties": "7.5.0",
    "@babel/plugin-proposal-export-default-from": "7.5.2",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.5.4",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.4",
    "autoprefixer": "9.6.1",
    "babel-eslint": "10.0.2",
    "babel-loader": "8.0.6",
    "babel-plugin-istanbul": "5.1.4",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "clean-webpack-plugin": "3.0.0",
    "compression-webpack-plugin": "3.0.0",
    "css-loader": "3.0.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "6.0.1",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-loader": "2.2.1",
    "eslint-plugin-flowtype": "3.11.1",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.2",
    "file-loader": "4.0.0",
    "flow-bin": "0.102.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.0.0",
    "image-webpack-loader": "5.0.0",
    "jsdom": "15.1.1",
    "lint-staged": "9.2.0",
    "mini-css-extract-plugin": "0.7.0",
    "mocha": "6.1.4",
    "mocha-junit-reporter": "1.23.0",
    "nock": "10.0.6",
    "nodemon": "1.19.1",
    "nyc": "14.1.1",
    "postcss-loader": "3.0.0",
    "prettier": "1.18.2",
    "react-test-renderer": "16.8.6",
    "rimraf": "2.6.3",
    "sinon": "7.3.2",
    "url-loader": "2.0.1",
    "webpack": "4.35.3",
    "webpack-cli": "3.3.5",
    "webpack-dev-server": "3.7.2"
  }
}
