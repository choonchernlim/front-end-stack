    "ci": "node ./scripts/ci",
    "reinstall": "yarn cache clean && yarn upgrade",
    "start": "webpack-dev-server --config webpack.dev.config.js --progress",
    "stats": "webpack --profile --json > stats.json",
    "flow": "flow"
  },
