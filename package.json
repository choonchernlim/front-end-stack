{
  "name": "front-end-stack",
  "private": true,
  "config": {
    "context_root": "/",
    "dist_uri": "/assets/",
    "src_dir_path": "src/",
    "test_dir_path": "src/**/*-spec.js",
    "dist_dir_path": "dist/assets/",
    "entry_file_path": "dist/index.html",
    "report_dir_path": "reports/"
  },
  "scripts": {
    "test": "node ./scripts/test",
    "test:watch": "nodemon --exec 'npm test'",
    "build": "webpack",
    "ci": "npm run ci:clean && npm run ci:test && npm run ci:coverage",
    "ci:clean": "node ./scripts/ci-clean",
    "ci:coverage": "node ./scripts/ci-coverage",
    "ci:test": "node ./scripts/ci-test",
    "reinstall": "npm cache clean && rimraf node_modules && npm install",
    "start": "webpack-dev-server --config webpack.dev.config.js"
  },
  "devDependencies": {
    "autoprefixer": "6.4.0",
    "babel-cli": "6.11.4",
    "babel-core": "6.13.2",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.12.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "clean-webpack-plugin": "0.1.10",
    "css-loader": "0.23.1",
    "enzyme": "2.4.1",
    "eslint": "3.3.1",
    "eslint-config-airbnb": "10.0.1",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "1.13.0",
    "eslint-plugin-jsx-a11y": "2.1.0",
    "eslint-plugin-react": "6.1.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "image-webpack-loader": "2.0.0",
    "isparta": "4.0.0",
    "jsdom": "9.4.2",
    "json-loader": "0.5.4",
    "mocha": "3.0.2",
    "mocha-junit-reporter": "1.12.0",
    "nock": "8.0.0",
    "node-sass": "3.8.0",
    "nodemon": "1.10.0",
    "postcss-loader": "0.10.1",
    "react-addons-test-utils": "15.3.0",
    "rimraf": "2.5.4",
    "roboto-fontface": "0.6.0",
    "sass-loader": "4.0.0",
    "sinon": "1.17.5",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "es6-promise": "3.2.1",
    "history": "2.1.2",
    "immutable": "3.8.1",
    "isomorphic-fetch": "2.2.1",
    "material-ui": "0.15.4",
    "radium": "0.18.1",
    "radium-grid": "2.0.1",
    "react": "15.3.0",
    "react-dom": "15.3.0",
    "react-redux": "4.4.5",
    "react-router": "2.6.1",
    "react-router-redux": "4.0.5",
    "react-tap-event-plugin": "1.0.0",
    "redux": "3.5.2",
    "redux-saga": "0.11.0",
    "reselect": "2.5.3"
  }
