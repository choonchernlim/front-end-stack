[ignore]
<PROJECT_ROOT>/node_modules/webpack-cli/.*
<PROJECT_ROOT>/node_modules/config-chain/.*

[options]
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.json

# To fix "Required module not found" error after ignoring dependencies above due to flow validation error
module.name_mapper='^webpack-cli$' -> '<PROJECT_ROOT>/src/flow/general-stub.js.flow'
module.name_mapper='^config-chain$' -> '<PROJECT_ROOT>/src/flow/general-stub.js.flow'

esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable

suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
